<%= form_with(model: location, local: true) do |form| %>

  <div class="field">
    <%= form.label :comuna %>
    <%= form.text_field :comuna %>
  </div>

  <div class="field">
    <%= form.label :region %>
    <%= form.text_field :region %>
  </div>

  <div class="field">
    <%= form.label :lat %>
    <%= form.text_field :lat %>
  </div>

  <div class="field">
    <%= form.label :long %>
    <%= form.text_field :long %>
  </div>

  <div class="field">
    <%= form.label :tipo_energia %>
    <%= form.text_field :tipo_energia %>
  </div>

  <div class="field">
    <%= form.label :medio_generacion %>
    <%= form.text_field :medio_generacion %>
  </div>

  <div class="field">
    <%= form.label :potencia_neta_mw %>
    <%= form.text_field :potencia_neta_mw %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
 
